{
  "name": "next-redirects-preact",
  "version": "3.0.1",
  "private": false,
  "description": "A small Redirect utility component for Next.js",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsdx build tsdx build --entry src/index.tsx --name index ",
    "prebuild": "rm -rf dist && next lint && tsc --noEmit -p tsconfig.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mzaien/next-redirects-preact"
  },
  "keywords": [
    "next",
    "next.js",
    "react",
    "redirect",
    "link",
    "preact",
    "router"
  ],
  "author": "Abdullah Mzaien <awmzaien@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mzaien/next-redirects-preact/issues"
  },
  "peerDependencies": {
    "next": ">= 9.3.6",
    "preact": ">= 10.5.12",
    "react": ">= 16.8.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "next": "^12.0.7",
    "preact": "^10.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
